{
  "expo": {
    "name": "Expo Blog",
    "slug": "expo-blog",
    "owner": "bacon",
    "version": "1.0.0",
    "orientation": "default",
    "scheme": "expoblog",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "ios": {
      "bundleIdentifier": "com.expoblog.app",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.expoblog.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "experiments": {
      "reactCompiler": true
    },
    "plugins": [
      "expo-dev-client",
      "expo-font",
      "expo-image",
      "expo-router",
      "expo-web-browser",
      "@bacons/apple-colors",
      "@bacons/apple-targets"
    ],
    "extra": {
      "eas": {
        "projectId": "b8e6b863-1e51-42d7-955f-481fdc9e61d7"
      }
    }
  }
}
